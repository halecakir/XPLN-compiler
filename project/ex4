sum:=0;
i:=0;

while(i < 10)
    j:=0;
    while(j < 10)
        k:=0;
        while(k < 10)
            sum := sum + 1;
            k:= k+1;
        endw;
        j:=j+1;
    endw;
    i:=i+1;
endw;

