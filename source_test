
x := 3;

fun f1 (a, b, c) 
x := 10;
y := 11;
X := 13;
return a + b * c;
endf;

fun f2 ()
a := 10;
b := 2; 
return a / b;
endf;

fun f3(n)
sum := 0;
for(i:=0; i <= n; i:=i+1)
    sum := sum + i;
endfor;
return sum;
endf;

A := f1(3,4,5);
output A;

B := f2();
output B;

input n;
X := f3(n);
output X;


